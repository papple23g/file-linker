{
  "name": "file-linker",
  "displayName": "File Linker",
  "description": "Hover and click to instantly open file paths using the Everything search engine. Requires Everything to be running.",
  "version": "1.0.0",
  "publisher": "papple23g",
  "repository": {
    "type": "git",
    "url": "https://github.com/papple23g/file-linker"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "file-linker.openFile",
        "title": "Open File with Everything"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run webpack:prod",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "webpack": "webpack --mode none",
    "webpack:prod": "webpack --mode production",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./dist/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^16.x.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^6.x.x",
    "@typescript-eslint/parser": "^6.x.x",
    "eslint": "^8.x.x",
    "ts-loader": "^9.5.2",
    "typescript": "^5.x.x",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "debug": "^4.4.0",
    "graphemer": "^1.4.0",
    "iconv-lite": "^0.6.3",
    "ignore": "^5.3.2",
    "natural-compare": "^1.4.0",
    "semver": "^7.7.1",
    "ts-api-utils": "^1.4.3"
  }
}
