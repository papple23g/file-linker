{
  "name": "file-linker",
  "displayName": "File Linker",
  "description": "Quickly open files by clicking text in square brackets while holding Alt key",
  "version": "0.0.1",
  "publisher": "user",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "file-linker.openFile",
        "title": "Open File with Everything"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "^16.x.x",
    "@typescript-eslint/eslint-plugin": "^6.x.x",
    "@typescript-eslint/parser": "^6.x.x",
    "eslint": "^8.x.x",
    "typescript": "^5.x.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "debug": "^4.4.0",
    "natural-compare": "^1.4.0",
    "ts-api-utils": "^1.4.3",
    "semver": "^7.7.1",
    "ignore": "^5.3.2",
    "graphemer": "^1.4.0"
  }
}
